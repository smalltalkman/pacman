msgid "  --holdver        Do not update VCS sources"
msgstr "  --holdver        VCS mənbələrini yeniləməmək"

#: scripts/makepkg.sh.in:911
#, sh-printf-format
msgid ""
"  --key <key>      Specify a key to use for %s signing instead of the default"
